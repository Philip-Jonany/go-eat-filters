{
  "name": "go/eat Filters",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  }
}


{
  "manifest_version": 3,
  "name": "go/eat Filters",
  "version": "1.0",
  "description": "Filters go/eat",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://eat.googleplex.com/*".
      ]
    }
  ]
}
